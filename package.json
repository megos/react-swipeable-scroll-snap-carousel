{
  "name": "react-swipeable-scroll-snap-carousel",
  "description": "A tiny swipeable carousel built with scroll snap for React",
  "version": "0.0.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/megos/react-swipeable-scroll-snap-carousel.git"
  },
  "homepage": "https://megos.github.io/react-swipeable-scroll-snap-carousel/",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.types.json",
    "preview": "vite preview",
    "lint": "rome check src examples",
    "format": "rome format src examples --write",
    "build:page": "vite -c vite.config.page.ts build"
  },
  "devDependencies": {
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "@vitejs/plugin-react": "^4.0.0",
    "cypress": "^12.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rome": "^12.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-css-injected-by-js": "^3.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  }
}
